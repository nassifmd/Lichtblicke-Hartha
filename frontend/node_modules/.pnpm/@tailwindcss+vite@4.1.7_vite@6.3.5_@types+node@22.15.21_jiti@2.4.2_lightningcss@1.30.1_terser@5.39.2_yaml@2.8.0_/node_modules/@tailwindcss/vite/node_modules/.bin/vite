#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules/vite/bin/node_modules:/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules/vite/node_modules:/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules:/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules/vite/bin/node_modules:/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules/vite/node_modules:/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules:/Applications/MAMP/htdocs/clients/Lichtblicke-Hartha/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.3.5_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_terser@5.39.2_yaml@2.8.0/node_modules/vite/bin/vite.js" "$@"
fi
